
public class As {

	public static void main(String[] args) {
		String str = "JavaProgramming";
		 str.chars().filter(x -> String.valueOf(x).equals("0")).count();
		System.out.println();
		char[] ch = new char[15];
		for(int i = 0; i < ch.length; i++ ) {
			ch[i] = str.charAt(i);
			System.out.print(ch[i] + " ");
		}
		
		
	}
}
