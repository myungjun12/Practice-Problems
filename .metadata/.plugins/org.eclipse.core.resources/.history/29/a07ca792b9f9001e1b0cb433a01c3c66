package practice1;

public class Ex7 {

	public static void main(String[] args) {
		
		Car car = null;
		FireEngine fe = new fireEngine();
		
		car = fe;
		
	}
}

class Car{
	String color;
	int door;
	void drive() {
		System.out.println("drive, Brrrr~~~");
	}
	void stop() {
		System.out.println("stop!!!");
	}
}
class FireEngine extends Car{
	void water() {
		System.out.println("water!!!");
	}
}
